<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.graduatestudent.mapper.BasicInformationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.graduatestudent.entity.BasicInformation">
        <id column="id" property="id" />
        <result column="cross_examination_situation" property="crossExaminationSituation" />
        <result column="test_state" property="testState" />
        <result column="undergraduate_university" property="undergraduateUniversity" />
        <result column="user_id" property="userId" />
        <result column="present_state" property="presentState" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, cross_examination_situation, test_state, undergraduate_university, user_id
    </sql>

</mapper>
